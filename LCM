#include<stdio.h>
#include<conio.h>
int 1cm(int,int);
Void main()
{
int a,b,result;
clrscr();
printf(" enter the numbers");
scand("%d%d",&a,&b);
result=1cm(a,b);
printf("the LCM of %d and %d is %d\n",a,b, result);
getch();
int 1cm(int a,int b)
{
static int common=1;
if (common%a==0&&common%b==0)
{
return common;
}
common++;
1cm(a,b);
return common;
}
}
